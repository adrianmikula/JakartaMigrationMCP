{
  "actorSpecification": 1,
  "fields": {
    "type": "object",
    "properties": {
      "timestamp": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp when the analysis was performed"
      },
      "tool": {
        "type": "string",
        "description": "Name of the MCP tool that was executed",
        "enum": [
          "analyzeJakartaReadiness",
          "detectBlockers",
          "recommendVersions",
          "createMigrationPlan",
          "verifyRuntime"
        ]
      },
      "projectPath": {
        "type": "string",
        "description": "Path to the project that was analyzed"
      },
      "status": {
        "type": "string",
        "enum": ["success", "error", "upgrade_required"],
        "description": "Status of the tool execution"
      },
      "readinessScore": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 1.0,
        "description": "Migration readiness score (0.0 to 1.0)"
      },
      "blockerCount": {
        "type": "integer",
        "minimum": 0,
        "description": "Number of blockers found"
      },
      "recommendationCount": {
        "type": "integer",
        "minimum": 0,
        "description": "Number of recommendations provided"
      },
      "riskScore": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 1.0,
        "description": "Risk assessment score (0.0 to 1.0)"
      }
    }
  },
  "views": {
    "overview": {
      "title": "Migration Analysis Results",
      "description": "Overview of all Jakarta migration analysis results",
      "transformation": {
        "fields": [
          "timestamp",
          "tool",
          "projectPath",
          "status",
          "readinessScore",
          "blockerCount",
          "recommendationCount",
          "riskScore"
        ],
        "desc": true
      },
      "display": {
        "component": "table",
        "properties": {
          "timestamp": {
            "label": "Date & Time",
            "format": "date"
          },
          "tool": {
            "label": "Tool",
            "format": "text"
          },
          "projectPath": {
            "label": "Project Path",
            "format": "text"
          },
          "status": {
            "label": "Status",
            "format": "text"
          },
          "readinessScore": {
            "label": "Readiness Score",
            "format": "number"
          },
          "blockerCount": {
            "label": "Blockers",
            "format": "number"
          },
          "recommendationCount": {
            "label": "Recommendations",
            "format": "number"
          },
          "riskScore": {
            "label": "Risk Score",
            "format": "number"
          }
        }
      }
    },
    "readinessAnalyses": {
      "title": "Readiness Analyses",
      "description": "Results from analyzeJakartaReadiness tool",
      "transformation": {
        "fields": [
          "timestamp",
          "projectPath",
          "readinessScore",
          "blockerCount",
          "recommendationCount",
          "riskScore"
        ],
        "omit": ["tool"],
        "desc": true
      },
      "display": {
        "component": "table",
        "properties": {
          "timestamp": {
            "label": "Date",
            "format": "date"
          },
          "projectPath": {
            "label": "Project",
            "format": "text"
          },
          "readinessScore": {
            "label": "Readiness",
            "format": "number"
          },
          "blockerCount": {
            "label": "Blockers",
            "format": "number"
          },
          "riskScore": {
            "label": "Risk",
            "format": "number"
          }
        }
      }
    }
  }
}

